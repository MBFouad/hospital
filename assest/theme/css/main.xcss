/* 
    Author     : Christian Pohl
    Description:  styles for normal tables with a huge amount of data, just add the class "dataTable"  to the table tag
*/

table.p td, table.p th{
padding: 2px 5px;
}

.dataTable {
font-size: 12px;
line-height: 1;
}

@media screen and (-webkit-min-device-pixel-ratio:0) { 
 .dataTable {
font-size: 11px;
}
}


table.dataTable {
	color: #darkgrey;
	border-collapse: collapse;
        width: 100%;
table-layout: fixed;
}

table.dataTable.lastChild, table.dataTable.lastChildL {
table-layout: auto;
}
.autoLayout{
table-layout: auto !important;
}

table.dataTable.lastChild tr td:last-child, table.dataTable.lastChild tr th:last-child{
width: 90%;
text-align: right;
}

table.dataTable.lastChildL tr td:last-child, table.dataTable.lastChildL tr th:last-child {
width: 90%;
text-align: left;
}

table.dataTable, table.dataTable caption {
	// border: 1px solid #CCC;
}

table.dataTable caption,table.dataTable th,table.dataTable td {
	border-left: 0;
	padding: 5px 10px;
        white-space: nowrap;
vertical-align: middle;
        
}

table.noWrap caption,table.noWrap th,table.noWrap td {
	border-left: 0;
        white-space: nowrap;
        
}


table.dataTable caption,table.dataTable thead th, table.dataTable tbody tr.header th {
	border-bottom: $darkGrey 2px solid;
	color: $darkGrey;
}

table.dataTable tfoot th,table.dataTable tfoot td{
border-top: $darkGrey 2px dashed;
}

table.dataTable tfoot th{
border-top: $mainColor 2px solid;
}

table.dataTable thead th, table.dataTable tbody tr.header th  {
	text-align: left;
}


table.dataTable tbody tr.odd {
	background-color: #F7F7F7;
	color: #666
}

table.dataTable tbody a {
	padding: 1px 2px;
	color: #333;
	text-decoration: none;
	border-bottom: 1px dotted #E63C1E;
}

table.dataTable tbody a:active,table.dataTable tbody a:hover,table.dataTable tbody a:focus,table.dataTable tbody a:visited {
	color: #666
}

table.dataTable tbody tr:hover {
	background-color: #EEE;
	color: #333
}

table.dataTable tbody tr:hover a {
	background-color: #FFF
}

table.dataTable tbody td+td+td+td a {
	color: [$mainColor * 0.4];
	font-weight: 700;
	border-bottom: 0
}

table.dataTable tbody td+td+td+td a:active,table.dataTable tbody td+td+td+td a:hover,table.dataTable tbody td+td+td+td a:focus,table.dataTable tbody td+td+td+td a:visited {
	color: #E63C1E
}

table.dataTable input {
width: auto;
    box-sizing: border-box;
    -webkit-box-sizing:border-box;
    -moz-box-sizing: border-box;
    margin: 0; 
}

table.dataTable select {
width: 100px;
}
table.dataTable.autoLayout select {
width: auto;
}

