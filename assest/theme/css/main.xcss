//flyout classes
.flyout{
display: inline;
}

.flyoutMenu{self{
    display: none;
position: absolute;
top: 40px;
right: 10px;
min-width: 50px;
border: 5px solid $mainColor;
border-radius: 10px;
background: #fff;
padding: 10px;}

self:after{
    content: "";
    position: absolute;
    top: -15px;
    right: 12.5px;
    border-style: solid;
    border-width: 0 20px 15px;
    border-color: $white transparent;
    display: block;
    width: 0;
    z-index: 1;    
}
self:before{
    content: "";
    position: absolute;
    top: -24px;
    right: 8.5px;
    border-style: solid;
    border-width: 0 24px 19px;
    border-color: $mainColor transparent;
    display: block;
    width: 0;
    z-index: 0;
}
self > ul{
color: $darkGrey;
list-style: none;
margin: 0;
padding: 0;
line-height: 20px;
}
}
